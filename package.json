    "eslint-plugin-prettier": "^4.2.1",
    "eslint-webpack-plugin": "^4.0.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "image-minimizer-webpack-plugin": "^3.8.1",
    "json5": "^2.2.3",
    "mini-css-extract-plugin": "^2.7.2",
