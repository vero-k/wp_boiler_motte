{
  "name": "honey",
  "version": "1.0.0",
  "description": "boilerplate js ts frontend esm webpack",
  "main": "src/scripts/app.ts",
  "scripts": {
    "lint": "eslint --ignore-path .eslintignore src//**/*.{js,ts,jsx,tsx} --quiet",
    "prettier:check": "prettier --list-different --ignore-path .prettierignore --config .prettierrc src//**/*.{js,ts,jsx,tsx,css,scss,sass}",
    "prettier:fix": "prettier --write --ignore-path .prettierignore --config .prettierrc src//**/*.{js,ts,jsx,tsx,css,scss,sass}",
    "test": "echo \"Error: no test specified\" && exit 1",
    "production": "webpack --config config/webpack.prod.config.js --mode=production",
    "build": "webpack --config config/webpack.dev.config.js --mode=development",
    "start": "webpack-dev-server --config config/webpack.dev.config.js --mode=development"
  },
  "author": "vero",
  "private": true,
  "license": "ISC",
  "keywords": [
    "boilerplate",
    "webpack",
    "es6"
  ],
  "repository": {
    "type": "git",
    "url": ""
  },
  "browserslist": [
    "last 2 versions",
    "> 5%",
    "not dead"
  ],
