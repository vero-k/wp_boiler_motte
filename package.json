    "@typescript-eslint/parser": "^5.54.1",
    "babel-loader": "^9.1.2",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "css-minimizer-webpack-plugin": "^4.2.2",
    "csv-loader": "^3.0.5",
    "eslint": "^8.35.0",
